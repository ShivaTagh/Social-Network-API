{
    "source": {
        "include": [
            "./lib",
            "./lib/util",
            "./lib/adapter",
            "./lib/transform",
            "package.json",
            "README.md"
        ],
        "exclude": [
            "./docs",
            "./node_modules",
            "./test",
            "./.git"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "docs"
    },
    "opts": {
        "template": "./node_modules/minami",
        "destination": "./docs/"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "default": {
            "outputSourceFiles": true,
            "layoutFile": "./config/src/layout-custom.tmpl",
            "staticFiles": {
                "include": [
                    "./config/src/style-custom.css",
                    "./config/src/icon.png"
                ]
            }
        },
        "logoFile": "",
        "cleverLinks": true,
        "monospaceLinks": false,
        "dateFormat": "dd.mm.yyyy",
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "systemName": "ADStore",
        "footer": "",
        "copyright": "Copyright Â© 2012-2015 The contributors to ADStore project.",
        "navType": "vertical",
        "theme": "flatly",
        "linenums": true,
        "collapseSymbols": false,
        "inverseNav": true,
        "protocol": "html://",
        "methodHeadingReturns": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    }
}
